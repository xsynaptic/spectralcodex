{
	"name": "@xsynaptic/spectralcodex",
	"type": "module",
	"version": "5.7.4",
	"scripts": {
		"astro": "astro",
		"dev": "astro dev",
		"build": "astro check && NODE_OPTIONS=--max-old-space-size=8192 astro build",
		"preview": "astro preview",
		"lint": "eslint",
		"lint-content": "eslint ./packages/content --config ./packages/content/eslint.config.mjs",
		"lint-content-demo": "eslint ./packages/content-demo --config ./packages/content-demo/eslint.config.mjs",
		"test": "vitest",
		"check-types": "tsc --noEmit --extendedDiagnostics",
		"host-images": "serve packages/content/media",
		"content-related": "pnpm --filter @spectralcodex/scripts content-related --root-path=$PWD --content-path=packages/content --cache-path=./node_modules/.astro",
		"content-schemas": "pnpm --filter @spectralcodex/scripts content-schemas --root-path=$PWD --content-path=packages/content",
		"content-validate": "pnpm --filter @spectralcodex/scripts content-validate --root-path=$PWD --content-path=packages/content",
		"map-divisions": "pnpm --filter @spectralcodex/scripts map-divisions --root-path=$PWD --cache-path ./node_modules/.astro/divisions --regions-path ./packages/content/collections/regions --output-path ./public/divisions --overture-url=s3://overturemaps-us-west-2/release/2025-10-22.0/",
		"map-spritesheet": "pnpm --filter @spectralcodex/scripts map-spritesheet --root-path=$PWD --temp-path ./temp/icons --output-path ./public/icons/map-icons",
		"opengraph-image": "pnpm --filter @spectralcodex/scripts opengraph-image --root-path=$PWD --content-path=packages/content --output-path=./public/0g",
		"deploy-static": "pnpm --filter @spectralcodex/scripts deploy-static --root-path=$PWD"
	},
	"license": "MIT",
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"not ie 11",
		"not ie_mob 11",
		"not dead"
	],
	"imports": {
		"#*": "./src/*"
	},
	"resolutions": {
		"vite": "6.4.1"
	},
	"dependencies": {
		"astro": "^5.16.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@astrojs/markdown-remark": "^6.3.9",
		"@astrojs/mdx": "^4.3.12",
		"@astrojs/node": "^9.5.1",
		"@astrojs/react": "^4.4.2",
		"@astrojs/rss": "^4.0.14",
		"@astrojs/sitemap": "^3.6.0",
		"@iconify/json": "^2.2.410",
		"@iconify/utils": "^3.0.2",
		"@keyv/sqlite": "^4.0.6",
		"@sindresorhus/slugify": "^3.0.0",
		"@spectralcodex/astro-build-logger": "workspace:*",
		"@spectralcodex/image-loader": "workspace:*",
		"@spectralcodex/image-open-graph": "workspace:*",
		"@spectralcodex/local-image-server": "workspace:*",
		"@spectralcodex/map-types": "workspace:*",
		"@spectralcodex/react-map-component": "workspace:*",
		"@spectralcodex/remark-img-group": "workspace:*",
		"@spectralcodex/scripts": "workspace:*",
		"@spectralcodex/unified-tools": "workspace:*",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@tanstack/eslint-plugin-query": "^5.91.2",
		"@turf/helpers": "^7.3.0",
		"@turf/turf": "^7.3.0",
		"@types/geojson": "^7946.0.16",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@xsynaptic/eslint-config": "^3.0.9",
		"alfaaz": "^1.1.0",
		"astro-auto-import": "^0.4.5",
		"astro-pagefind": "^1.8.5",
		"astro-seo": "^0.8.4",
		"chalk": "^5.6.2",
		"eslint": "^9.39.1",
		"eslint-plugin-astro": "^1.5.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"exiftool-vendored": "^33.5.0",
		"geojson": "^0.5.0",
		"globals": "^16.5.0",
		"keyv": "^5.5.4",
		"ky": "^1.14.0",
		"maplibre-gl": "5.11.0",
		"nanoid": "^5.1.6",
		"ohash": "^2.0.11",
		"p-limit": "^7.2.0",
		"p-memoize": "^8.0.0",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"react-compiler-runtime": "19.1.0-rc.3",
		"rehype-wrap-cjk": "^1.0.3",
		"remeda": "^2.32.0",
		"sharp": "^0.34.5",
		"tailwindcss": "^4.1.17",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0",
		"unified": "^11.0.5",
		"url-join": "^5.0.0",
		"vite": "6.4.1",
		"vitest": "^4.0.14",
		"yaml": "^2.8.1",
		"zod": "^3.25.76",
		"zx": "^8.8.5"
	},
	"packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
