{
	"name": "@xsynaptic/spectralcodex",
	"type": "module",
	"version": "6.6.2",
	"scripts": {
		"astro": "astro",
		"dev": "tsx packages/scripts/src/dev-server/index.ts",
		"build": "astro check && NODE_OPTIONS=--max-old-space-size=8192 astro build",
		"preview": "astro preview",
		"lint": "eslint",
		"lint-strict": "ESLINT_STRICT=1 eslint",
		"lint-content": "pnpm --filter @spectralcodex/content lint",
		"lint-content-demo": "eslint ./packages/content-demo --config ./packages/content-demo/eslint.config.mjs",
		"test": "vitest",
		"test:image-server": "vitest run --config vitest.integration.config.ts",
		"check-types": "tsc --noEmit --extendedDiagnostics",
		"content-related": "pnpm --filter @spectralcodex/scripts content-related --root-path=$PWD",
		"content-schemas": "pnpm --filter @spectralcodex/scripts content-schemas --root-path=$PWD --output-path=packages/content",
		"content-validate": "pnpm --filter @spectralcodex/scripts content-validate --root-path=$PWD",
		"map-divisions": "pnpm --filter @spectralcodex/scripts map-divisions --root-path=$PWD --overture-url=s3://overturemaps-us-west-2/release/2026-01-21.0/",
		"media-orphans": "pnpm --filter @spectralcodex/scripts media-orphans --root-path=$PWD",
		"og-image": "pnpm --filter @spectralcodex/scripts og-image --root-path=$PWD",
		"deploy-site": "pnpm --filter @spectralcodex/scripts deploy-site --root-path=$PWD",
		"deploy-app": "pnpm --filter @spectralcodex/scripts deploy-app --root-path=$PWD",
		"deploy-media": "pnpm --filter @spectralcodex/scripts deploy-media --root-path=$PWD",
		"deploy-og": "pnpm --filter @spectralcodex/scripts deploy-og --root-path=$PWD",
		"cache-warm": "pnpm --filter @spectralcodex/scripts image-server-warm --root-path=$PWD",
		"cache-warm-new": "pnpm --filter @spectralcodex/scripts image-server-warm-new --root-path=$PWD"
	},
	"license": "MIT",
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"not ie 11",
		"not ie_mob 11",
		"not dead"
	],
	"imports": {
		"#*": "./src/*"
	},
	"dependencies": {
		"astro": "6.0.0-beta.11",
		"react": "^19.2.4",
		"react-dom": "^19.2.4"
	},
	"devDependencies": {
		"@astrojs/markdown-remark": "7.0.0-beta.6",
		"@astrojs/mdx": "5.0.0-beta.7",
		"@astrojs/node": "10.0.0-beta.4",
		"@astrojs/react": "^5.0.0-beta.3",
		"@astrojs/rss": "4.0.15-beta.3",
		"@astrojs/sitemap": "3.6.1-beta.3",
		"@spectralcodex/astro-build-logger": "workspace:*",
		"@spectralcodex/image-loader": "workspace:*",
		"@spectralcodex/react-map-component": "workspace:*",
		"@spectralcodex/remark-img-group": "workspace:*",
		"@spectralcodex/scripts": "workspace:*",
		"@spectralcodex/shared": "workspace:*",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/eslint-plugin-query": "^5.91.4",
		"@turf/bbox": "^7.3.4",
		"@turf/boolean-intersects": "^7.3.4",
		"@turf/buffer": "^7.3.4",
		"@turf/center": "^7.3.4",
		"@turf/centroid": "^7.3.4",
		"@turf/distance": "^7.3.4",
		"@turf/helpers": "^7.3.4",
		"@turf/truncate": "^7.3.4",
		"@types/geojson": "^7946.0.16",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"@unpic/astro": "^1.0.2",
		"@unpic/core": "^1.0.3",
		"@xsynaptic/eslint-config": "^3.1.4",
		"@xsynaptic/image-server": "workspace:*",
		"@xsynaptic/unified-tools": "3.0.0",
		"alfaaz": "^1.1.0",
		"astro-auto-import": "^0.5.1",
		"astro-pagefind": "^1.8.5",
		"astro-seo": "^1.1.0",
		"dotenv": "^17.2.4",
		"eslint": "^9.39.2",
		"eslint-plugin-astro": "^1.5.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"exiftool-vendored": "^35.9.0",
		"geojson": "^0.5.0",
		"geokdbush": "^2.0.1",
		"globals": "^17.3.0",
		"kdbush": "^4.0.2",
		"maplibre-gl": "^5.18.0",
		"p-limit": "^7.3.0",
		"p-memoize": "^8.0.0",
		"prettier": "^3.8.1",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"react-compiler-runtime": "^19.1.0-rc.3",
		"rehype-wrap-cjk": "^1.0.9",
		"remeda": "^2.33.6",
		"sharp": "^0.34.5",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0",
		"unified": "^11.0.5",
		"unpic": "^4.2.2",
		"url-join": "^5.0.0",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"yaml": "^2.8.2",
		"zod": "^4.3.6"
	},
	"packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
