{
	"name": "@xsynaptic/spectralcodex",
	"type": "module",
	"version": "5.7.0",
	"scripts": {
		"astro": "astro",
		"dev": "astro dev",
		"build": "astro check && NODE_OPTIONS=--max-old-space-size=8192 astro build",
		"preview": "astro preview",
		"lint": "eslint",
		"lint-content": "eslint ./packages/content --config ./packages/content/eslint.config.mjs",
		"lint-content-demo": "eslint ./packages/content-demo --config ./packages/content-demo/eslint.config.mjs",
		"test": "vitest",
		"check-types": "tsc --noEmit --extendedDiagnostics",
		"clean-build-images": "tsx --env-file=.env ./scripts/clean-build-images.ts",
		"host-images": "serve packages/content/media",
		"content-related": "pnpm --filter @spectralcodex/scripts content-related --root-path=$PWD --content-path=packages/content --cache-path=./node_modules/.astro",
		"content-schemas": "pnpm --filter @spectralcodex/scripts content-schemas --root-path=$PWD --content-path=packages/content",
		"content-validate": "pnpm --filter @spectralcodex/scripts content-validate --root-path=$PWD --content-path=packages/content",
		"map-divisions": "pnpm --filter @spectralcodex/scripts map-divisions --root-path=$PWD --cache-path ./node_modules/.astro/divisions --regions-path ./packages/content/collections/regions --output-path ./public/divisions --overture-url=s3://overturemaps-us-west-2/release/2025-09-24.0/",
		"map-spritesheet": "pnpm --filter @spectralcodex/scripts map-spritesheet --root-path=$PWD --temp-path ./temp/icons --output-path ./public/icons/map-icons",
		"opengraph-image": "pnpm --filter @spectralcodex/scripts opengraph-image --root-path=$PWD --content-path=packages/content --output-path=./public/0g",
		"deploy-hybrid": "tsx --env-file=.env ./scripts/deploy-hybrid.ts",
		"deploy-static": "pnpm --filter @spectralcodex/scripts deploy-static --root-path=$PWD"
	},
	"license": "MIT",
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"not ie 11",
		"not ie_mob 11",
		"not dead"
	],
	"imports": {
		"#*": "./src/*"
	},
	"resolutions": {
		"vite": "6.3.6"
	},
	"dependencies": {
		"astro": "^5.15.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@astrojs/markdown-remark": "^6.3.8",
		"@astrojs/mdx": "^4.3.8",
		"@astrojs/node": "^9.5.0",
		"@astrojs/react": "^4.4.0",
		"@astrojs/rss": "^4.0.13",
		"@astrojs/sitemap": "^3.6.0",
		"@iconify/json": "^2.2.401",
		"@iconify/utils": "^3.0.2",
		"@keyv/sqlite": "^4.0.6",
		"@sindresorhus/slugify": "^3.0.0",
		"@spectralcodex/astro-build-logger": "workspace:*",
		"@spectralcodex/image-loader": "workspace:*",
		"@spectralcodex/image-open-graph": "workspace:*",
		"@spectralcodex/local-image-server": "workspace:*",
		"@spectralcodex/map-types": "workspace:*",
		"@spectralcodex/react-map-component": "workspace:*",
		"@spectralcodex/remark-img-group": "workspace:*",
		"@spectralcodex/scripts": "workspace:*",
		"@spectralcodex/unified-tools": "workspace:*",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.16",
		"@tanstack/eslint-plugin-query": "^5.91.2",
		"@turf/helpers": "^7.2.0",
		"@turf/turf": "^7.2.0",
		"@types/geojson": "^7946.0.16",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@xsynaptic/eslint-config": "^3.0.6",
		"alfaaz": "^1.1.0",
		"astro-auto-import": "^0.4.5",
		"astro-pagefind": "^1.8.5",
		"astro-seo": "^0.8.4",
		"chalk": "^5.6.2",
		"eslint": "^9.38.0",
		"eslint-plugin-astro": "^1.4.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"exiftool-vendored": "^31.2.0",
		"geojson": "^0.5.0",
		"globals": "^16.4.0",
		"keyv": "^5.5.3",
		"ky": "^1.13.0",
		"maplibre-gl": "^5.10.0",
		"nanoid": "^5.1.6",
		"p-limit": "^7.2.0",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"react-compiler-runtime": "19.1.0-rc.3",
		"rehype-wrap-cjk": "^1.0.3",
		"remeda": "^2.32.0",
		"sharp": "^0.34.4",
		"tailwindcss": "^4.1.16",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2",
		"unified": "^11.0.5",
		"url-join": "^5.0.0",
		"vite": "6.3.6",
		"vitest": "^4.0.4",
		"yaml": "^2.8.1",
		"zod": "^3.25.76",
		"zx": "^8.8.5"
	},
	"packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
