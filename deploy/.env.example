# Server Configuration
# Copy to .env and fill in values
#
# Storage volume mount point
# DigitalOcean volumes use UNDERSCORES, not hyphens!
# Example: /mnt/volume_sfo3_01 (correct)
# NOT:     /mnt/volume-sfo3-01 (wrong)
STORAGE_PATH=/mnt/volume_xxx_01

# Umami secrets (generate with: openssl rand -hex 32)
UMAMI_DB_PASSWORD=
UMAMI_APP_SECRET=
